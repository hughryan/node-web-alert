{
	"name": "node-web-alert",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "https://github.com/hughryan/node-web-alert.git",
	"author": "hughryan",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"attach": "docker attach webalert || exit 0",
		"build": "docker build -t webalert .",
		"start": "docker run -d -it --name webalert webalert",
		"stop": "docker stop webalert",
		"poststop": "docker rm webalert",
		"prod": "node index.js"
	},
	"devDependencies": {
		"eslint": "7.26.0",
		"eslint-config-airbnb-base": "14.2.1",
		"eslint-plugin-import": "2.23.2"
	},
	"dependencies": {
		"@slack/web-api": "6.2.3",
		"@slack/webhook": "6.0.0",
		"discord.js": "12.5.3",
		"dotenv": "9.0.2",
		"mkdirp": "1.0.4",
		"pixelmatch": "5.2.1",
		"pngjs": "6.0.0",
		"puppeteer": "9.1.1",
		"ramda": "0.27.1"
	}
}
